<section class="sign-up">
  <form class="form-signin" action="/users/register" method="POST">
    <h2 class="form-signin-heading">Please sign up</h2>

    <label for="inputEmail" class="sr-only">Email address</label>
    <input type="email" id="inputEmail" name="email" class="form-control input" placeholder="Email address" required autofocus>

    <label for="inputUsername" class="sr-only">Username</label>
    <input type="text" id="inputUsername" name="username" class="form-control input" placeholder="Username" required>

    <label for="inputPassword" class="sr-only">Password</label>
    <input type="password" id="inputPassword" name="password" class="form-control input" placeholder="Password" required>

    <label for="inputConfirmPassword" class="sr-only">Confirm Password</label>
    <input type="password" id="inputConfirmPassword" name="confirmationPassword" class="form-control input" placeholder="Confirm Password" required>

    <br/>

    <button class="btn btn-lg btn-default btn-block" type="submit">Sign up</button>
  </form>
</section>
